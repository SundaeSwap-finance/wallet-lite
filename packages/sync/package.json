{
  "name": "@sundaeswap/sync",
  "version": "0.0.1",
  "main": "./dist/cjs/exports/index.js",
  "module": "./dist/esm/exports/index.js",
  "types": "./dist/types/exports/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/exports/index.js",
      "require": "./dist/cjs/exports/index.js",
      "types": "./dist/types/exports/index.d.ts"
    },
    "./react-components": {
      "import": "./dist/esm/exports/react-components.js",
      "require": "./dist/cjs/exports/react-components.js",
      "types": "./dist/types/exports/react-components.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "test": "bun test --preload=./setup-tests.ts --coverageThreshold=0.8 --root=./src/__tests__",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "clean": "rm -rf ./dist",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts,.tsx' --out-dir './dist/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts,.tsx' --out-dir 'dist/cjs' --source-maps",
    "types": "tsc --project ./tsconfig.build.json",
    "build": "bun run clean && bun run types && bun run build:esm && bun run build:cjs",
    "watch": "bun run clean && bun run build && bun run build:esm --watch"
  },
  "dependencies": {
    "@babel/cli": "^7.24.7",
    "@cardano-sdk/core": "^0.33.0",
    "@cardano-sdk/dapp-connector": "^0.12.18",
    "@cardano-sdk/util": "^0.15.1",
    "@fabianbormann/cardano-peer-connect": "^1.2.17",
    "@koralabs/adahandle-sdk": "^1.5.4",
    "@sundaeswap/asset": "^0.7.1",
    "@sundaeswap/fraction": "0.5.9",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-error-boundary": "^4.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/lodash": "^4.17.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@happy-dom/global-registrator": "^14.12.0",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/libsodium-wrappers-sumo": "^0.7.8",
    "@types/node": "^20.14.2",
    "@types/node-fetch": "^2.6.11",
    "fetch-mock": "^9.11.0",
    "happy-dom": "^14.12.0",
    "cross-env": "^7.0.3",
    "typescript": "^5.4.5"
  }
}
